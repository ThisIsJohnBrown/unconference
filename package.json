{
  "name": "unconference",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8086",
    "build": "vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "emulators:saved": "kill -9 $(lsof -ti tcp:8080); firebase emulators:start --import=./emulator-data --export-on-exit",
    "emulators": "kill -9 $(lsof -ti tcp:8080); firebase emulators:start",
    "test:open": "cypress open",
    "test:emulate": "cross-env FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" yarn test:open",
    "generate": "node generators.js",
    "test:firestore": "mocha tests/firestore/**/*.js",
    "testwhy": "why-is-node-running ./node_modules/.bin/mocha tests/firestore/**/*.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "firebase": "^8.0.1",
    "vue": "^2.6.11",
    "vue-async-computed": "3.9.0",
    "vue-awesome-countdown": "^1.1.2",
    "vue-router": "^3.2.0",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0",
    "vuexfire": "^3.2.4"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
